{
  "name": "api",
  "version": "1.0.0",
  "description": "An unopinionated express starter kit with support for es6+ features using babel",
  "private": false,
  "repository": "https://github.com/benmalcom/express-babel-starter",
  "author": "Ben ikeji <benjaminikeji@gmail.com>",
  "main": "dist/app.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/app",
    "start:dev": "nodemon src/app --exec \"npm run lint && node -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && mkdir dist && babel src/ -s -d dist",
    "test": "mocha --exit --require babel-register test/",
    "lint": "esw src"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.20.1",
    "config": "^1.31.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "dotenv": "^8.1.0",
    "ejs": "^2.7.1",
    "express": "^4.18.2",
    "express-validator": "^6.2.0",
    "mongoose": "^5.7.1",
    "morgan": "^1.9.1",
    "nodemon": "^1.19.2",
    "rimraf": "^2.7.1",
    "validatorjs": "^3.17.1"
  },
  "keywords": [
    "babel",
    "es6",
    "es7",
    "starter",
    "boilerplate",
    "express-babel-boilerplate",
    "nodejs",
    "js",
    "mocha",
    "eslint",
    "babel-cli",
    "seed",
    "express-babel-seed",
    "es2015",
    "es2016"
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-mocha": "^4.12.1",
    "eslint-watch": "^3.1.5",
    "mocha": "^4.1.0",
    "supertest": "^3.4.2"
  }
}
